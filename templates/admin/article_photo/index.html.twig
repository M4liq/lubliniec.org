<!DOCTYPE html>

<title>ArticlePhoto index</title>

{% block body %}
    <h1>ArticlePhoto index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Path</th>
                <th>Section_no</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for article_photo in article_photos %}
            <tr>
                <td>{{ article_photo.id }}</td>
                <td>{{ article_photo.path }}</td>
                <td>{{ article_photo.sectionNo }}</td>
                <td>
                    <a href="{{ path('article_photo_show', {'id': article_photo.id}) }}">show</a>
                    <a href="{{ path('article_photo_edit', {'id': article_photo.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('article_photo_new') }}">Create new</a>
{% endblock %}
